<html>
    <head>
        <title> List of Artcles</title>
    </head>
    <body>
        <div>
            <h1>Article List</h1>

  <% for(var i=0; i<articles.length; i++) { %>
    <h1> <%= articles[i].title %> </h1>
    <p>
      <%= articles[i].user%>
    </p>
    <h3><%= articles[i].content%></h3>
    <% } %>

        </div>    
    </body>
    </html>